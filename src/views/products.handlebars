<div>
  {{#each docs}}
    <div>
      <h2>{{this.title}}</h2>
      <p>{{this.description}}</p>
      <p>{{this.category}}</p>
      <h3>Precio: $ {{this.price}}</h3>
    </div>
    <hr />
  {{/each}}
</div>

{{#if hasPrevPage}}
  <a href="/api/products?page={{prevPage}}&limit={{limit}}&sort={{sort}}">Página anterior</a>
{{/if}}

{{#if hasNextPage}}
  <a href="/api/products?page={{nextPage}}&limit={{limit}}&sort={{sort}}">Página siguiente</a>
{{/if}}

